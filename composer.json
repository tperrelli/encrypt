{
    "name": "tperrelli/encrypt",
    "description": "A simple package to encrypt data",
    "type": "package",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.5.0|~5.6",
        "illuminate/contracts": "~5.5.0|~5.6"
    },
    "authors": [
        {
            "name": "Tiago Perrelli",
            "email": "tiagoyg@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tperrelli\\Encrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tperrelli\\Encrypt\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tperrelli\\Encrypt\\Providers\\AppServiceProvider"
            ]
        }
    }
}
